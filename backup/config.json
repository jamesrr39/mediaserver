{
  "version": 1,
  "requiredVariables": ["SERVER_BASE_URL"],
  "listingUrl": "${SERVER_BASE_URL}/api/graphql?query={mediaFiles{videos{relativePath,participantIds,fileSizeBytes,hashValue,fileModTime,fileMode},pictures{relativePath,participantIds,fileSizeBytes,hashValue,fileModTime,fileMode},tracks{relativePath,participantIds,fileSizeBytes,hashValue,fileModTime,fileMode}},people{name,id}}",
  "files": [{
    "modTimeKey": "fileModTime",
    "sizeKey": "fileSizeBytes",
    "relativePathKey": "relativePath",
    "fileModeKey": "fileMode",
    "downloadUrlPattern": "${SERVER_BASE_URL}/api/files/${HASH_VALUE}",
    "forEach": ["data", "mediaFiles", "videos"],
    "variableMappings": {
      "HASH_VALUE": "hashValue"
    }
  }, {
    "modTimeKey": "fileModTime",
    "sizeKey": "fileSizeBytes",
    "relativePathKey": "relativePath",
    "fileModeKey": "fileMode",
    "downloadUrlPattern": "${SERVER_BASE_URL}/api/files/${HASH_VALUE}",
    "forEach": ["data", "mediaFiles", "pictures"],
    "variableMappings": {
      "HASH_VALUE": "hashValue"
    }
  }, {
    "modTimeKey": "fileModTime",
    "sizeKey": "fileSizeBytes",
    "relativePathKey": "relativePath",
    "fileModeKey": "fileMode",
    "downloadUrlPattern": "${SERVER_BASE_URL}/api/files/${HASH_VALUE}",
    "forEach": ["data", "mediaFiles", "tracks"],
    "variableMappings": {
      "HASH_VALUE": "hashValue"
    }
  }]
}
